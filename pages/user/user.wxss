/**index.wxss**/
@import "../common/header.wxss";

/* 头部 */
.search-img{
    width: 10%;
    height: 60rpx;
    margin-left: 9rpx;
}

.add-feedback-img{
    width: 10%;
    height: 60rpx;
    margin-right: 9rpx;
}

/* 用户信息 */
.userinfo {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 40rpx;
}

.userinfo-avatar {
    width: 128rpx;
    height: 128rpx;
    margin: 20rpx;
    border-radius: 50%;
}

.userinfo-nickname {
    color: #aaa;
}

.usermotto {
    margin-top: 50px;
}

.btns {
    margin-top: 50px;
}

/* navigator */
.navigator-box{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-around;
    /* align-content: space-around; */
    width: 100%;
    margin-top: 80rpx;
    padding: 50rpx;
}
.navi-btn{
    width: 60%;
    margin: 30rpx 20rpx;
}
